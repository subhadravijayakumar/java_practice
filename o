1.length of last word-51
class Solution {
    public int lengthOfLastWord(String s) {

        int first = 0;
        for(int i = s.length()-1 ; i>0;i--){

            if(s.charAt(i)!=' ' && s.charAt(i-1) == ' ' ){

                 first = i;
                 break;

            }

        }

        String temp  = s.substring(first).trim();

        return temp.length();
    }
}
2.Maximum sum of a subarray of size k (sliding window)
class Solution {
    public int maxSubarraySum(int[] arr, int k) {
        int n = arr.length;
        if (n < k) return -1;  // edge case

        // Compute sum of first window
        int currSum = 0;
        for (int i = 0; i < k; i++) {
            currSum += arr[i];
        }

        int maxSum = currSum;

        // Slide the window
        for (int i = k; i < n; i++) {
            currSum = currSum - arr[i - k] + arr[i];  // remove left, add right
            maxSum = Math.max(maxSum, currSum);
        }

        return maxSum;
    }
}
